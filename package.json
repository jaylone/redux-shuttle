{
  "name": "redux-shuttle",
  "version": "0.3.5",
  "description": "A tool for bundling reducers, action types and actions when using redux, aesthetic toppings fro your redux meal.",
  "main": "es/index.js",
  "scripts": {
    "start": "node server.js",
    "build:umd:prod": "npm run eslint && cross-env BABEL_ENV=prod webpack --config webpack.build.config.js -p",
    "build:es": "babel src --out-dir es",
    "prepublish": "npm run build:es && npm run build:umd:prod",
    "eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaylone/redux-shuttle.git"
  },
  "keywords": [
    "redux",
    "saga",
    "redux-saga",
    "react",
    "react-redux",
    "side effects",
    "shuttle",
    "redux-shuttle",
    "redux side effects"
  ],
  "author": "jaylone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaylone/redux-shuttle/issues"
  },
  "homepage": "https://github.com/jaylone/redux-shuttle#readme",
  "dependencies": {
    "ramda": "^0.22.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.9.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.1",
    "redux-saga": "^0.15.4",
    "rimraf": "^2.5.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
